<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Button fx:id="branchBtn" mnemonicParsing="false" onAction="#showBranchCommits" onMouseClicked="#showBranchCommits" prefHeight="25.0" prefWidth="65.0" text="Button" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.singleBranch.SingleBranchController">
   <contextMenu>
      <ContextMenu>
        <items>
            <MenuItem mnemonicParsing="false" onAction="#branchCheckout" text="Checkout" />
            <MenuItem mnemonicParsing="false" onAction="#branchDelete" text="Delete " />
          <MenuItem mnemonicParsing="false" onAction="#branchReset" text="Reset" />
            <MenuItem mnemonicParsing="false" onAction="#mergeBranch" text="Merge" />
        </items>
      </ContextMenu>
   </contextMenu>
</Button>
